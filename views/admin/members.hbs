<section>
  <div>
    <h3 class="mt-2" style="text-align: center;">All Members</h3>
  </div>
  <div class="container mt-3">
    <table class="table">
      <thead>
        <tr>

          <th scope="col">Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Mobile Number</th>
          <th scope="col">Monthly Payment</th>
          <th scope="col">Payable Amount</th>
          <th scope="col">Payement Done</th>
          <th scope="col">Balance To Pay</th>
          <th scope="col">Status</th>
          <th scope="col">Gifted Date</th>
          <th scope="col">Payement Status</th>
         


        </tr>
      </thead>
      <tbody>
        {{#each members}}
        <tr>

          <td>{{this.fname}}</td>
          <td>{{this.Lname}}</td>
          <td>{{this.Mobile}}</td>
          <td>{{this.payment}}</td>
          <td>{{this.payable}}</td>
          <td style="color:green;">{{this.paid}}</td>
          <td style="color: red;">{{this.balance}}</td>
          
          <td style="color:green;">{{this.status}}</td>
          <td style="color:blue;">{{this.date}}</td>
          <td>{{this.payementstatus}}</td>
          

        </tr>
        {{/each}}

      </tbody>
    </table>

  </div>
</section>